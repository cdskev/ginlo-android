    <!-- The ginlo app default light theme -->
    <style name="GinloDefaultLight" parent="GinloDefaultThemeBaseLight">
        <item name="textScaleFactor">@dimen/textScaleFactor</item>
        <!-- All text sizes that are set programmatically by ScreenDesignUtil class -->
        <item name="baseTextSize">@dimen/baseTextSize</item>
        <item name="messageTextSize">@dimen/messageTextSize</item>
        <item name="messageEditTextSize">@dimen/messageEditTextSize</item>
        <item name="labelTextSize">@dimen/labelTextSize</item>
        <item name="miniLabelTextSize">@dimen/miniLabelTextSize</item>
        <item name="statusTextSize">@dimen/statusTextSize</item>
        <item name="chooserTextSize">@dimen/chooserTextSize</item>
        <item name="mainTitleTextSize">@dimen/mainTitleTextSize</item>
        <item name="subTitleTextSize">@dimen/subTitleTextSize</item>
        <item name="tanViewTextSize">@dimen/tanViewTextSize</item>
        <item name="tanEditTextSize">@dimen/tanEditTextSize</item>
        <item name="mediumTitleTextSize">@dimen/mediumTitleTextSize</item>
        <item name="notesTextSize">@dimen/notesTextSize</item>
        <!-- Others -->
        <item name="android:windowContentTransitions">true</item>
        <item name="android:windowAllowEnterTransitionOverlap">true</item>
        <item name="android:windowAllowReturnTransitionOverlap">true</item>
        <item name="android:windowSharedElementEnterTransition">@android:transition/move</item>
        <item name="android:windowSharedElementExitTransition">@android:transition/move</item>
        <item name="android:scrollbarThumbVertical">@drawable/scrollbar</item>
        <item name="android:scrollbarAlwaysDrawVerticalTrack">false</item>
        <item name="android:scrollbarFadeDuration">1000</item>
        <item name="android:scrollbarDefaultDelayBeforeFade">0</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:editTextStyle">@style/EditTextStyle</item>
        <item name="android:spinnerStyle">@style/SpinnerStyle</item>
        <item name="android:textViewStyle">@style/RobotoTextViewStyle</item>
        <item name="android:buttonStyle">@style/ButtonStyle</item>
        <item name="drawerArrowStyle">@style/DrawerArrowStyle</item>
        <item name="android:itemTextAppearance">@style/MenuItemTextAppearance</item>
    </style>

