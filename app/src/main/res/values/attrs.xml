    <attr name="messageEditTextSize" format="dimension" />
    <attr name="labelTextSize" format="dimension" />
    <attr name="miniLabelTextSize" format="dimension" />
    <attr name="statusTextSize" format="dimension" />
    <attr name="chooserTextSize" format="dimension" />
    <attr name="mainTitleTextSize" format="dimension" />
