<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaskImageView">
        <attr name="mask" format="integer" />
    </declare-styleable>

    <declare-styleable name="FloatingActionButton">
        <attr name="fab_colorPressed" format="color" />
        <attr name="fab_colorDisabled" format="color" />
        <attr name="fab_colorNormal" format="color" />
        <attr name="fab_icon" format="reference" />
        <attr name="fab_title" format="string" />
        <attr name="fab_stroke_visible" format="boolean" />
        <attr name="fab_size" format="enum">
            <enum name="normal" value="0" />
            <enum name="mini" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="StyledView">
        <attr name="styledTextSize" format="string" />
        <attr name="styledTextColor" format="string" />
        <attr name="styledTextColorHint" format="string" />
        <attr name="styledBackground" format="string" />
        <attr name="styledBackgroundColorFilter" format="string" />
        <attr name="styledIconColor" format="string" />
        <attr name="styledButtonType" format="string" /><!-- normal borderless -->
        <attr name="styledEditTextNoLineBG" format="boolean" />
    </declare-styleable>

    <attr name="mainColor" format="color" />
    <attr name="main50Color" format="color" />
    <attr name="mainContrastColor" format="color" />
    <attr name="mainContrast50Color" format="color" />
    <attr name="mainContrast80Color" format="color" />
    <attr name="blackColor" format="color" />
    <attr name="whiteColor" format="color" />
    <attr name="transparentColor" format="color" />
    <attr name="alertColor" format="color" />
    <attr name="actionColor" format="color" />
    <attr name="actionContrastColor" format="color" />
    <attr name="actionSecondaryColor" format="color" />
    <attr name="secureColor" format="color" />
    <attr name="secureContrastColor" format="color" />
    <attr name="mediumColor" format="color" />
    <attr name="mediumContrastColor" format="color" />
    <attr name="insecureColor" format="color" />
    <attr name="insecureContrastColor" format="color" />
    <attr name="interferingColor" format="color" />
    <attr name="interferingContrastColor" format="color" />
    <attr name="overlayColor" format="color" />
    <attr name="overlayContrastColor" format="color" />
    <attr name="fabColor" format="color" />
    <attr name="fabIconColor" format="color" />
    <attr name="sendButtonColor" format="color" />
    <attr name="sendButtonIconColor" format="color" />
    <attr name="unreadBubbleColor" format="color" />
    <attr name="unreadBubbleContrastColor" format="color" />
    <attr name="toolbarColor" format="color" />
    <attr name="fabOverviewColor" format="color" />
    <attr name="fabIconOverviewColor" format="color" />
    <attr name="chatItemColor" format="color" />
